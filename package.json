{
  "name": "nuxt-v2-motorsafety",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt --mode=development",
    "dev:host": "HOST=0 yarn dev",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate --fail-on-error",
    "generate:local": "ulimit -s 65500; node --stack-size=65500 --max-old-space-size=3072 ./node_modules/.bin/nuxt generate --fail-on-error",
    "generate:copy": "node -r esm ./generator/copy.js",
    "import:wp": "node -r esm ./generator/import_wp.js",
    "import:dealers": "node -r esm ./generator/import_dealers.js",
    "dist:serve": "http-server -p 3000 ./dist",
    "lint": "eslint --ext .js,.vue .",
    "lint:fix": "yarn lint --fix",
    "deploy": "./deploy.sh"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@aws-amplify/ui-vue": "^1.0.2",
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-brands-svg-icons": "^5.13.1",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@nuxt/content": "^1.14.0",
    "@nuxtjs/apollo": "^4.0.1-rc.1",
    "@nuxtjs/axios": "^5.11.0",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "@nuxtjs/sitemap": "^2.4.0",
    "@zxing/library": "^0.17.0",
    "aws-amplify": "^3.3.20",
    "aws-appsync": "^4.0.2",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "bootstrap-vue": "^2.15.0",
    "cloudinary-vue": "1.1.1",
    "date-fns": "^2.14.0",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.10.3",
    "lodash": "^4.17.21",
    "nuxt": "^2.14.7",
    "nuxt-fontawesome": "^0.4.0",
    "promised-sqlite3": "^1.2.0",
    "vue-intercom": "^1.0.14",
    "vue-owl-carousel": "^2.0.3",
    "vue-select": "^3.10.3",
    "vue-slick-carousel": "^1.0.6",
    "vue-tel-input": "^4.4.0",
    "vue2-google-maps": "^0.10.7",
    "vue2-perfect-scrollbar": "^1.5.0",
    "vuelidate": "^0.7.5"
  },
  "devDependencies": {
    "@nuxt/types": "^2.13.3",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/svg": "^0.1.11",
    "babel-eslint": "^10.1.0",
    "checksum": "^1.0.0",
    "cross-env": "^7.0.2",
    "csvtojson": "^2.0.10",
    "eslint": "^7.29.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "fs-extra": "^10.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "lokijs": "^1.5.12",
    "node-json-db": "^1.4.1",
    "node-sass": "^4.14.1",
    "nuxt-jsonld": "^1.5.0",
    "sass-loader": "^10.1.1"
  }
